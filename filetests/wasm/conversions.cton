; Test code generation for WebAssembly type conversion operators.
test compile

set is_64bit=1
isa intel haswell

function %i32_wrap_i64(i64) -> i32 {
ebb0(v0: i64):
    v1 = ireduce.i32 v0
    return v1
}

function %i64_extend_s_i32(i32) -> i64 {
ebb0(v0: i32):
    v1 = sextend.i64 v0
    return v1
}

function %i64_extend_u_i32(i32) -> i64 {
ebb0(v0: i32):
    v1 = uextend.i64 v0
    return v1
}
