function test(i32) {
    ebb0(v0: i32):    ; dominates(0)
        brz v0, ebb1  ; dominates(1,3,4,5)
        jump ebb2     ; dominates(2)
    ebb1:
        jump ebb3
    ebb2:
        brz v0, ebb4
        jump ebb5
    ebb3:
        jump ebb4
    ebb4:
        brz v0, ebb3
        jump ebb5
    ebb5:
        brz v0, ebb4
        return
}
